<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <servlet>
      <servlet-name>SynService1</servlet-name>
      <servlet-class>com.suoyasoft.boh.servlet.SynService1</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>
  <servlet>
      <servlet-name>SynService2</servlet-name>
      <servlet-class>com.suoyasoft.boh.servlet.SynService2</servlet-class>
      <load-on-startup>0</load-on-startup>
  </servlet>
  <servlet>
      <servlet-name>SynService3</servlet-name>
      <servlet-class>com.suoyasoft.boh.servlet.SynService3</servlet-class>
      <load-on-startup>1</load-on-startup>
  </servlet>
  
  <servlet-mapping>
      <servlet-name>SynService1</servlet-name>
      <url-pattern>/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>SynService2</servlet-name>
      <url-pattern>/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
      <servlet-name>SynService3</servlet-name>
      <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>
